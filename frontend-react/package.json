    "@chakra-ui/react": "^2.10.9",
    "@chakra-ui/utils": "^2.2.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.85.9",
    "@testing-library/dom": "^10.4.1",
    "axios": "^1.11.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.12",
    "msw": "^1.3.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-paginate": "^8.3.0",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "util": "^0.12.5",
    "web-streams-polyfill": "^4.1.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@chakra-ui|@emotion|framer-motion|axios)/)"
    ],
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs",
      "^@chakra-ui/react$": "<rootDir>/src/test-utils/chakra-mock.js",
      "^react-icons/fa$": "<rootDir>/src/test-utils/icon-mock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/src/test-utils/css-mock.js"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1"
  }
}
